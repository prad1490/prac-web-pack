{
  "name": "grs-poc",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "del-cli public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch --mode development",
    "build:prod": "del-cli public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack  --mode production --config webpack.config.prod.js --progress --profile --bail && del-cli 'assets/app/**/*.js' 'assets/app/**/*.ngsummary.json' 'assets/app/**/*.ngstyle.*'",
    "build:css": "node-sass ./scss/grs-bundle.scss -o ./public/stylesheets/",
    "build:css-prod": "node-sass ./scss/grs-bundle.scss -o ./public/stylesheets/ --output-style compressed"
  },
